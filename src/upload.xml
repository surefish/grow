<struts>

    <package name="new-upload" namespace="/goUpload" extends="struts-default">
    <!-- 去上传页面 -->
       <action  name="goUpload" method="goUpload" class="com.begin.action.FileUploadAction" >
       <result name="success">/WEB-INF/web/upload/list.jsp</result>  
       </action>
       
       
       <action  name="myUpload" method="myUpload" class="com.begin.action.FileUploadAction" >
       <result name="success">/WEB-INF/web/upload/list.jsp</result>  
       </action>
    
        <!-- 去导入excl页面-->
       <action  name="goExcl" method="goExcl" class="com.begin.action.FileUploadAction" >
       <result name="success">/WEB-INF/web/importstudent/importEDate.jsp</result>  
       </action>
         
      <action  name="importDataforSql" method="importDataforSql" class="com.begin.action.FileUploadAction" >
       <result name="success">/WEB-INF/web/importstudent/importEDate.jsp</result>  
       </action>
       
    </package>
        
</struts>
